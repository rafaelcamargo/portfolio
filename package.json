{
  "name": "portfolio",
  "version": "0.1.0",
  "description": "My Personal Website",
  "main": "src/scripts/app.js",
  "scripts": {
    "build": "npm run build:clear && npm run build:blog && webpack && npm run parse:assets",
    "build:blog": "triven build",
    "build:clear": "rm -rf ./dist",
    "backup:blog:drafts": "cp -rf ./src/scripts/blog/drafts ~/Documents/Personal/MD-format",
    "parse:assets": "node ./src/scripts/base/services/assets",
    "format": "eslint ./src/**/*.js",
    "start": "npm run build:blog && webpack-dev-server",
    "serve:blog": "npm run build:clear && npm run build:blog && http-server ./dist/blog",
    "test": "jest",
    "deploy": "rm -f ./firebase.json && node ./firebase.js && firebase deploy --non-interactive"
  },
  "keywords": [
    "rafael",
    "camargo",
    "programmer",
    "designer"
  ],
  "author": "Rafael Camargo <hello@rafaelcamargo.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@glorious/triven": "^0.12.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "codecov": "^3.6.1",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.17.0",
    "file-system": "^2.2.2",
    "firebase-tools": "^7.11.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "mini-css-extract-plugin": "^0.8.2",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prerender-spa-plugin": "^3.4.0",
    "prop-types": "^15.7.2",
    "redux": "^4.0.5",
    "shortid": "^2.2.15",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "terser-webpack-plugin": "^2.3.1",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-merge": "^4.2.2"
  },
